  <groupId>org.jetbrains.idea</groupId>
  <artifactId>IntelliLang-xml</artifactId>

  <name>Maven module for IntelliLang-xml</name>
  <description>Maven integration for module IntelliLang-xml</description>
  <build>
    <sourceDirectory>${idea.home}/plugins/IntelliLang/xml-support</sourceDirectory>
    <resources>
      <resource>
        <directory>${idea.home}/plugins/IntelliLang/xml-support</directory>
        <excludes>
          <exclude>**/*.java</exclude>
          <exclude>**/*.groovy</exclude>
        </excludes>
      </resource>
    </resources>
