<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>parent.nine.904</artifactId>
    <groupId>org.jetbrains.idea</groupId>
    <version>9.0.4</version>
    <relativePath>../../../pom.xml</relativePath>
  </parent>
  <groupId>org.jetbrains.idea</groupId>
  <artifactId>debugger-openapi</artifactId>

  <name>Maven module for debugger-openapi</name>
  <description>Maven integration for module debugger-openapi</description>
  <build>
    <sourceDirectory>${idea.home}/java/debugger/openapi/src</sourceDirectory>
    <resources>
      <resource>
        <directory>${idea.home}/java/debugger/openapi/src</directory>
        <excludes>
          <exclude>**/*.java</exclude>
          <exclude>**/*.groovy</exclude>
        </excludes>
      </resource>
    </resources>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.jetbrains.idea</groupId>
      <artifactId>execution-openapi</artifactId>

    </dependency>
    <dependency>
      <groupId>org.jetbrains.idea</groupId>
      <artifactId>jsp-openapi</artifactId>
    </dependency>

    <dependency>
      <groupId>com.sun</groupId>
      <artifactId>tools</artifactId>
      <version>${java.version}</version>
      <scope>system</scope>
      <systemPath>${java.tools.jar}</systemPath>
    </dependency>
  </dependencies>

</project>
